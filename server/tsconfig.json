{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@routes/*": ["routes/*"],
      "@services/*": ["services/*"],
      "@models/*": ["models/*"]
    }
	},
  "exclude": ["node_modules"],
	"include": ["src/**/*"]
}
